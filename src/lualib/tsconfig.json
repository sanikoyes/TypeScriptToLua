{
    "compilerOptions": {
        "outDir": "../../dist/lualib",
        "target": "esnext",
        "lib": ["esnext"],
        "types": ["../../language-extensions", "lua-types/5.4"],
        "skipLibCheck": true,

        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "tstl": {
        "luaLibImport": "none",
        "noHeader": true
    }
}
